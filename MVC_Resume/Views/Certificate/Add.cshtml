
@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Add New Certificate</h2>
<br />
<form class="form-group" method="post">
    <label>Title</label>
    <input type="text" name="CertificateTitle" class="form-control" />
    <label>Organization</label>
    <input type="text" name="CertificateOrganization" class="form-control" />
    <label>Time</label>
    <input type="text" name="CertificateTime" class="form-control" />
    <label>Date</label>
    <input type="text" name="CertificateDate" class="form-control" />
    <label>Url</label>
    <input type="text" name="CertificateUrl" class="form-control" />
    <br />
    <button class="btn btn-success">SAVE</button>
</form>