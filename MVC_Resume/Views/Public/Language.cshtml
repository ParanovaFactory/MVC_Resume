@using MVC_Resume.Models.Entity
@model List<Tbl_Languages>


<div class="my-auto">
    <h2 class="mb-5">Languages</h2>
    @foreach (var item in Model)
    {
        <ul class="fa-ul mb-0">
            <li>
                <i class="fa-li fa fa-trophy text-warning"></i>
                @item.LanguageDescription
            </li>
        </ul>
    }
</div>