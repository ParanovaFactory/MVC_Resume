@using MVC_Resume.Models.Entity
@model List<Tbl_Educations>

<div class="my-auto">
    <h2 class="mb-5">Education</h2>
    @foreach (var item in Model)
    {
        <div class="resume-item d-flex flex-column flex-md-row mb-5">
            <div class="resume-content mr-auto">
                <h3 class="mb-0">@item.EducationSubtitle</h3>
                <div class="subheading mb-3">@item.EducationTitle</div>
                <div>@item.EducationDescription</div>
                <p>@item.EducationAvg</p>
            </div>
            <div class="resume-date text-md-right">
                <span class="text-primary">@item.EducationDate</span>
            </div>
        </div>
    }
</div>